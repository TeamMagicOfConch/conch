{
  "$schema": "https://turbo.build/schema.v2.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "**/*.{ts,tsx,js,jsx}",
        "vitest.config.ts"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**",
        "node_modules/.cache/metro/**"
      ]
    },
    "build:simulator": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}