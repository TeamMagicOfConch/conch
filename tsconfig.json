{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@api": [
        "./packages/api/dist/index.d.ts"
      ],
      "@api/*": [
        "./packages/api/src/*"
      ],
      "@conch/*": [
        "./apps/conch/*"
      ],
      "@admin/*": [
        "./apps/admin/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
